<div id="<%= dom_id vote %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Issue:</strong>
    <%= vote.issue_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Participant:</strong>
    <%= vote.participant_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Participant name:</strong>
    <%= vote.participant_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Value:</strong>
    <%= vote.value %>
  </div>
</div>
